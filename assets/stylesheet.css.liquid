#navigation { 
  width: 100%;
  position: relative;
}

nav { 
  display: inline-block;
  float: right;
  padding-right: 131px;
}


ul#main-nav { 
  list-style: none; 
  text-align: left;
  margin-bottom: 0;
  width: 100%;
  padding-top: 14px;
}


ul#main-nav li { 
  display: inline-block; 
  vertical-align: middle;
}


ul#main-nav li a { 
  font-family: {{ settings.navigation-font}}; 
  font-weight: {{ settings.nav-weight }}; 
  font-size: {{ settings.nav_size }}; 
  display: block; 
  padding: 0 10px; 
  text-transform: {{ settings.navigation-transform}};
  letter-spacing: 1px;
}


/* HIGHLIGHT ACTIVE LINK */
ul#main-nav li a.current { 
  color: {{ settings.homepage-pre-scroll-navigation-active-ros }}! important;
}

/* NO LINK UNDERLINE */
ul#main-nav li a:hover {
  text-decoration: none;
}

/* DOWNDOWN INDICATOR CHEVRONS */
ul#main-nav li.dropdown > a:after {
  font-family: 'FontAwesome'; 
  content: "\f107";
  margin-left: 10px;
  line-height: initial;
}

/* DROPDOWN LEVEL 1 */
ul#main-nav li ul.submenu {
  display: none;
  position: absolute;
  z-index: 5;
  -webkit-transition: background-color 1000ms linear;
  -moz-transition: background-color 1000ms linear;
  -o-transition: background-color 1000ms linear;
  -ms-transition: background-color 1000ms linear;
  transition: background-color 1000ms linear;
  width: 200px;
}

/* SHOW DROPDOWN ON HOVER */
ul#main-nav li.dropdown:hover ul.submenu {
  display: block;
}

/* INDEX DROPDOWN STATIC */
.index header ul#main-nav li ul { 
  {% if settings.transparent-header %}
  background: transparent;
  {% else %}
  background: {{ settings.homepage-header-pre-scroll-background }}; 
  {% endif %}
  border: 0 none;
}

/* INDEX DROPDOWN SCROLLING */
.index .coverheader ul#main-nav li ul  { 
  background: {{ settings.homepage-header-background }}; 
  border: 1px solid {{ settings.dotted_color }};
  border-left: 0 none;
  border-right: 0 none;  
}

/* REST OF SITE DROPDOWN SCROLLING */
header ul#main-nav li ul { 
  background: {{ settings.homepage-header-pre-scroll-background-ros }}; 
  border: 1px solid {{ settings.dotted_color }};
  border-left: 0 none;
  border-right: 0 none;  
}

/* REST OF SITE DROPDOWN SCROLLING */
.coverheader ul#main-nav li ul  { 
  background: {{ settings.homepage-header-background-ros }}; 
  -webkit-transition: background-color 1000ms linear;
  -moz-transition: background-color 1000ms linear;
  -o-transition: background-color 1000ms linear;
  -ms-transition: background-color 1000ms linear;
  transition: background-color 1000ms linear;
}

/* DROPDOWN LEVEL 1 STYLES */
ul#main-nav li ul.submenu li{
  line-height: 30px! important;
  margin: 0; 
  padding: 0;
  width: 100%;
  white-space: nowrap;
  background: transparent! important;
  min-width: 220px;
  float: left;
}

/* DROPDOWN LEVEL 1 LINK STYLES */
ul#main-nav li ul.submenu li a{
  text-align: left;
  text-transform: {{ settings.dropdown-transform}}! important;
  font-weight: normal;
  font-size: {{ settings.dropdown-size }}! important;
}

/* NESTED MENU */
.submenu li.nest {
  position: relative;
}

/* NESTED MENU INDICATOR */
ul#main-nav li ul.submenu li.nest:after {
  position: absolute;
  top: 0px;
  right: 30px;
  font-family: 'FontAwesome'; 
  content: "\f105";
}

/* VISIBLE NESTED MENU */
.submenu li.nest:hover > ul.nested {
  display: block; 
}

/* HIDDEN NESTED MENU */
.nested {
  display: none;
  position: absolute;
  left: 200px;
  top: 0;  
}

.nested li a {
  text-transform: {{ settings.dropdown-transform}}! important;
  font-weight: normal;
  font-size: {{ settings.dropdown-size }}! important;
}


ul.megamenu {
  display: none;
  position: absolute;
  left: 0;
  z-index: 2;
  line-height: initial;
  width: 100%;
  -webkit-transition: background-color 1000ms linear;
  -moz-transition: background-color 1000ms linear;
  -o-transition: background-color 1000ms linear;
  -ms-transition: background-color 1000ms linear;
  transition: background-color 1000ms linear;  
}


