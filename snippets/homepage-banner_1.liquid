<section id="homepage-pages-one" class="parallax parallax-window no-fouc scroll"  data-parallax="scroll" data-image-src="{{ 'parallax-background-1.jpg' | asset_url }}">    
  <article class="row">
    <div class="pages-left-content desktop-12 tablet-6 mobile-3">
      {% if settings.pages-1-content == 'text' %}
      <div class="parallax-text">{% if settings.pages-1-link != blank %}<a href="{{ settings.pages-1-link }}">{{ settings.pages-1-text }}</a>{% else %}{{ settings.pages-1-text }}{% endif %}</div>
      {% elsif settings.pages-1-content == 'page-content' %}
      {{ pages[settings.pages-1].content }}     
      {% else %}
      {% endif %}
    </div>
  </article>
</section>
<div class="clear"></div>
